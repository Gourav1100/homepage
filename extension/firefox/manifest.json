{
  "manifest_version": 2,
  "name": "Custom New Tab",
  "version": "1.0",
  "description": "Extension to add a custom home page",
  "icons": {
    "48": "icons/icon_48.png",
    "96": "icons/icon_96.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "author": "Gourav Bidhuri",
  "permissions": ["tabs"],
  "browser_specific_settings": {
    "gecko": {
      "id": "gauravb1100@gmail.com"
    }
  }
}
